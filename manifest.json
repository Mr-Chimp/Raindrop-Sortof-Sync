{
  "manifest_version": 3,
  "name": "Raindrop Sync",
  "version": "1.0",
  "description": "A fairly simple extension that syncs a Raindrop collection with Chrome's bookmarks",
  "permissions": ["storage","bookmarks"],
  "action": {
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "options_page": "options.html"
}
